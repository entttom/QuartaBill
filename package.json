{
  "name": "rechnung-generator",
  "version": "1.0.0",
  "description": "Automatischer Rechnungsgenerator fÃ¼r Quartalsabrechnungen",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "scripts": {
    "electron": "electron .",
    "electron-dev": "set ELECTRON_IS_DEV=true && electron .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:mac": "npm run build && electron-builder --mac"
  },
  "build": {
    "appId": "com.rechnung.generator",
    "productName": "Rechnung-Generator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "public/electron.js",
      "node_modules/**/*"
    ],
    "compression": "store",
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "sign": null,
      "certificateFile": null,
      "certificatePassword": null
    }
  },
  "author": "Rechnung Generator",
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "date-fns": "^2.30.0",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "wait-on": "^7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
